metadata:
  name: web-from-partial
  labels:
    name: web-from-partial
    app: test-partials
spec:
  replicas: 1
  selector:
    matchLabels:
      name: web-from-partial
      app: test-partials
  template:
    metadata:
      labels:
        name: web-from-partial
        app: test-partials
    spec: 
      containers:
      - name: partial-pod
        image: busybox
        imagePullPolicy: IfNotPresent
        command: ["sleep", "8000"]
